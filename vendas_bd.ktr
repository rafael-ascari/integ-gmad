    <sql>    SELECT
      F.CNPJ AS ESTAB,
      N.IDNOTA,
      NI.SEQITEM,
      N.EMISSAO, 
      N.ENTRADASAIDA,
      DECODE(N.OPERACAO,'E',N.ENTRADASAIDA,N.EMISSAO) DATA,
      N.HORAGRAV DATAHORA,  
      N.IDTIPOOPER,
      P.CNPJF AS VENDEDOR,
      (SELECT V.CNPJF FROM V_SIMPLEPESS V WHERE V.IDPESS = (SELECT MAX(CI.IDVENDEDORCART)
                                                           FROM NOTACARRINHO NC
                                                          INNER JOIN CARRINHOITEM CI ON CI.IDCARRINHO = NC.IDCARRINHO
                                                          WHERE NC.ESTAB = N.ESTAB
                                                            AND NC.IDNOTA = N.IDNOTA)) VENDEDORCAR,

      C.CNPJF AS CLIENTE,
      NI.IDITEM,
      COALESCE(NIC.PROMOCAO,'N') PROMOCAO,
      NI.QTDTRIB * CASE N.IDTIPOOPER WHEN 'DV' THEN -1 ELSE 1 END QUANTIDADE,
      NI.VALCONT * CASE N.IDTIPOOPER WHEN 'DV' THEN -1 ELSE 1 END VLRCONT,
      (COALESCE(NI.DESCONTO,0) + COALESCE(NI.DESCITEM,0)) * CASE N.IDTIPOOPER WHEN 'DV' THEN -1 ELSE 1 END DESCONTO,
      COALESCE(NI.VALFRETE,0) * CASE N.IDTIPOOPER WHEN 'DV' THEN -1 ELSE 1 END FRETE,
      COALESCE(NIC.VALSERVICO,0) * CASE N.IDTIPOOPER WHEN 'DV' THEN -1 ELSE 1 END SERVICO,
      COALESCE(NI.DESPESAS,0) - COALESCE(NIC.JUROS,0) * CASE N.IDTIPOOPER WHEN 'DV' THEN -1 ELSE 1 END DESPESAS,
      COALESCE(NIC.JUROS,0) * CASE N.IDTIPOOPER WHEN 'DV' THEN -1 ELSE 1 END JUROS,
      COALESCE(NI.SEGURO,0) * CASE N.IDTIPOOPER WHEN 'DV' THEN -1 ELSE 1 END SEGURO,
      COALESCE(NI.CUSTOUNIT,0) * CASE N.IDTIPOOPER WHEN 'DV' THEN -NI.QTDTRIB ELSE NI.QTDTRIB END CUSTO,
      COALESCE(NL.CUSTOMEDIO,0) * CASE N.IDTIPOOPER WHEN 'DV' THEN -NI.QTDTRIB ELSE NI.QTDTRIB END CUSTOMEDIO,
      COALESCE(NL.PMZ,0) * CASE N.IDTIPOOPER WHEN 'DV' THEN -NI.QTDTRIB ELSE NI.QTDTRIB END PMZ,
      (NI.VALCONT - (NI.CUSTOUNIT * NI.QTDTRIB)) * CASE N.IDTIPOOPER WHEN 'DV' THEN -1 ELSE 1 END LUCROBRUTO,
      ICMS.VALIMP VICMS,
      COALESCE(ICMS.VICMSUFDEST,0) + COALESCE(ICMS.VICMSUFREMET,0) VDIFAL,
      COALESCE(ICMS.VFCPUFDEST,0) VFCP,
      ICMS.ICMSST VST,
      PIS.VALORPIS VPIS,
      COFINS.VALORCOFINS VCOFINS
    FROM NOTA N
      INNER JOIN FILIAL F ON F.ESTAB = N.ESTAB
      LEFT JOIN NOTAITEM NI ON NI.ESTAB = N.ESTAB AND NI.IDNOTA = N.IDNOTA
      LEFT JOIN NOTALUCRO NL ON NL.ESTAB = NI.ESTAB AND NL.IDNOTA = NI.IDNOTA AND NL.SEQITEM = NI.SEQITEM
      LEFT JOIN NOTAITEMCP NIC ON NIC.ESTAB = NI.ESTAB AND NIC.IDNOTA = NI.IDNOTA AND NIC.SEQITEM = NI.SEQITEM
      LEFT JOIN NOTAICMS ICMS ON ICMS.ESTAB = NI.ESTAB AND ICMS.IDNOTA = NI.IDNOTA AND ICMS.SEQITEM = NI.SEQITEM
      LEFT JOIN NOTAPIS PIS ON PIS.ESTAB = NI.ESTAB AND PIS.IDNOTA = NI.IDNOTA AND PIS.SEQITEM = NI.SEQITEM
      LEFT JOIN NOTACOFINS COFINS ON COFINS.ESTAB = NI.ESTAB AND COFINS.IDNOTA = NI.IDNOTA AND COFINS.SEQITEM = NI.SEQITEM
      LEFT JOIN V_SIMPLEPESS C ON C.IDPESS = N.IDPESS
      LEFT JOIN V_SIMPLEPESS P ON P.IDPESS = NI.IDVENDEDOR
    WHERE DECODE(N.OPERACAO,'E',N.ENTRADASAIDA,N.EMISSAO) >= SYSDATE-${dias}
      AND N.IDTIPOOPER IN ('DV','EF','VD')
      AND N.SITUACAO IN (5,10)
      AND NOTAPASSOUCAIXA(N.ESTAB, N.IDNOTA, N.IDNOTACONF) = 'S'
    ORDER BY N.ESTAB, N.IDNOTA, NI.SEQITEM </sql>
