    <connection>mcp_origem</connection>
    <sql>SELECT
  F.CNPJ,
  I.IDITEM,
  I.DESCRICAO,
  I.TIPO,
  I.NCM,
  I.NROCODBARR AS CODBAR1,
  I.NROCODBARR1 AS CODBAR2,
  I.NROCODBARR2 AS CODBAR3,
  P.CODREF,
  R.INATIVO,
  COALESCE(D.DESCRICAO,'N/I') AS DEPARTAMENTO,
  COALESCE(S.DESCRICAO,'N/I') AS SECAO,
  COALESCE(G.DESCRICAO,'N/I') AS GRUPO,
  COALESCE(B.DESCRICAO,'N/I') AS SUBGRUPO,
  COALESCE(M.DESCRICAO,'N/I') AS MARCA  ,
  I.UNIDADE,
  CASE 
    WHEN COALESCE(GP.SUBGRUPOGMAD, 'N/I') &lt;&gt; 'N/I' THEN GP.SUBGRUPOGMAD
    WHEN COALESCE(GG.GRUPOGMAD, 'N/I')  &lt;&gt; 'N/I' THEN GG.GRUPOGMAD
    WHEN COALESCE(GS.SECAOGMAD, 'N/I')  &lt;&gt; 'N/I' THEN GS.SECAOGMAD
    ELSE 'Outros'
  END AS GRUPOGMAD

FROM ITEMESTAB E
  INNER JOIN ITEM I ON I.ESTAB = E.ESTABITEM AND I.IDITEM = E.IDITEM
  INNER JOIN FILIAL F ON F.ESTAB = E.ESTAB
  LEFT JOIN ITEMEMP P ON P.EMPRESA = F.EMPRESA AND P.ESTABITEM = E.ESTABITEM AND P.IDITEM = E.IDITEM
  LEFT JOIN ITEMCATEGORIA C ON C.ESTAB = I.ESTAB AND C.IDITEM = I.IDITEM  
  LEFT JOIN DEPARTAMENTO D ON D.ESTAB = C.ESTAB AND D.IDDEPTO = C.IDDEPTO
  LEFT JOIN SECAO S ON S.ESTAB = C.ESTAB AND S.IDSECAO = C.IDSECAO
  LEFT JOIN GRUPOITEM G ON G.ESTAB = C.ESTAB AND G.IDGRUPOITEM = C.IDGRUPOITEM
  LEFT JOIN SUBGRUPO B ON B.ESTAB = C.ESTAB AND B.IDSUBGRUPO = C.IDSUBGRUPO
  LEFT JOIN MARCA M ON M.IDMARCA = C.IDMARCA
  LEFT JOIN ITEMREPOSICAO R ON R.ESTAB = E.ESTAB AND R.ESTABITEM = E.ESTABITEM AND R.IDITEM = E.IDITEM
  LEFT JOIN SECAO_U GS ON GS.ESTAB = C.ESTAB AND GS.IDSECAO = C.IDSECAO
  LEFT JOIN GRUPOITEM_U GG ON GG.ESTAB = C.ESTAB AND GG.IDGRUPOITEM = C.IDGRUPOITEM
  LEFT JOIN SUBGRUPO_U GP ON GP.ESTAB = C.ESTAB AND GP.IDSUBGRUPO = C.IDSUBGRUPO
WHERE E.USAMIX = 'S'
  AND EXISTS (
  SELECT 0
  FROM NOTAITEM NI 
    INNER JOIN NOTA N ON N.ESTAB = NI.ESTAB AND N.IDNOTA = NI.IDNOTA
  WHERE NI.ESTAB = E.ESTAB
    AND NI.IDITEM = E.IDITEM
    AND N.EMISSAO >= SYSDATE-${dias}
)</sql>
